<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="countries-list-flow">
    <default-activity>countriesListView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>CountriesList</managed-bean-name>
      <managed-bean-class>com.redsamurai.view.beans.Countries</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="countriesListView">
      <page>/fragments/countriesListView.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
